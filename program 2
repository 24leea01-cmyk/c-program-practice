#include<stdio.h>
  int main(){
      int q1,q2,q3,q4,q5;
      int p1,p2,p3,p4,p5;
      int total=0,discount=0;
      printf("enter quantites\n");
      scanf("%d %d %d %d %d",&q1,&q2,&q3,&q4,&q5);
      printf("enter price\n");
      scanf("%d %d %d %d %d",&p1,&p2,&p3,&p4,&p5);
      
      total=(q1*p1)+(q2*p2)+(q3*p3)+(q4*p4)+(q5*p5);
      printf("total=%d\n",total);
      
      if(total>=500)
      {
          discount=total*0.05;
      }
      else if(total>=1000)
      {
          discount=total*0.1;
      }
      else if(total<0)
      {
          printf("error\n");
      }
      else if(total==0)
      {
          printf("0");
      }
      int final=0;
      int gst=total*0.05;
      final=total-discount+gst;
       printf("%d\n",discount);
        printf("%d\n",final);
         printf("%d\n",total);
          printf("%d\n",gst);
       
      
      return 0;
  }
