#include<stdio.h>
  int main(){
      int age,dis;
      char name[30];
     
      scanf("%d %d",&age,&dis);
      printf("%d %d\n",age,dis);
      scanf(" %s",&name);
      printf("%s\n",name);
      int ticketprice=dis*3;
      int final=0;
      int discount=0;
        if(age==0)
      {
          printf("0");
      } 
      else if(age<5&&age>0)
      {
          printf("free\n");
      }
      else if(age>=60)
      {
          discount=ticketprice*0.5; 
      }
      else if (age<0)
      {
          printf("invalid\n");
      }
      
       final=ticketprice-discount;
      printf("discount=%d\n",discount);
      printf("final=%d\n",final);
      return 0;
      
  }
